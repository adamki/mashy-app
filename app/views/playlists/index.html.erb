<div class="row" id ="center-horizontal">

  <% if current_user %>
  <h3 class ="center"><%= current_user.name %>'s PlayLists</h3>

  <div class="col l3 s1">
  </div>

  <div class="col l6 s10 z-depth-5 center">
    <%=  %>
    <% @spotify_service.find_users_playlists.each do |playlist| %>
      <%= link_to "show", playlist_path(playlist.id) %>
    <% end %>
  </div>

  <div class="col l3 s1">
  </div>
  <% end %>
</div>
